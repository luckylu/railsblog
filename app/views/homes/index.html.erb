

<% @posts.each do |p| %>

<div class="jumbotron">
Title: <%= p.title %> </br>
Content: <%= p.content %></br>
<%= link_to "show details", post_path(p) %>
</br>
</div>
<% end %>

<div class="flickr_pagination">
    <div class="page_info">
      <%= page_entries_info @posts %>
    </div>
    <%= will_paginate @posts, :container => false %>

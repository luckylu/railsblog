<!DOCTYPE HTML>
<html>
<head>
	<title> Users index </title>
</head>
<body>
  <%= render "admin/shared/head" %>
  <%= render "admin/shared/sidebar" %>

<div class="container">
<% @user.errors.full_messages.each do |m| %>
<%= m %>
<% end %>
<%= form_for @user, url: {action:"update"} do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :admin %>
<%= f.check_box :admin %>
<%= f.submit "Update" %>

<% end %>


</div>
</body>

</html>
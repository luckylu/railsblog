<!DOCTYPE HTML>
<html>
<head>
<title>Posts index</title>
</head>
<body>
<% if user_signed_in? %>
  <%= current_user.email %>
  <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
<% end %>
</br></br>
<%= link_to "New", new_admin_post_path %> | <%= link_to "View site", root_path %> | <%= link_to "Admim Dashboard", admin_dashboard_path %>
</br></br>
<% n = 1 %>
<% @posts.each do |p| %>

Post <%= n %> </br> 
Title: <%= p.title %> </br>
Content: <%= p.content %></br>
<%= link_to "Edit", edit_admin_post_path(p) %> | <%= link_to "Show", controller: "/homes", action: "show", id: p.id %> | <%= link_to "Delete", admin_post_path(p), method: :delete %>
</br>
</br>
<% n += 1 %>
<% end %>


</body>
</html>
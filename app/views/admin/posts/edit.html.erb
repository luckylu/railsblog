<!DOCTYPE HTML>
<html>
<head>
<title>New Post</title>
</head>
<body>
<%= render "admin/shared/head" %>


<%= render "admin/shared/sidebar" %>

<div class="container">
<%= form_for @post, url: {action: "update"} do |f| %>
  <%= f.text_field :title %> </br>
  <%= f.text_area :content %>
  <%= f.submit "Update" %>
<% end %>
</div>
</body>
</html>